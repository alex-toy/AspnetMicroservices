services:
  articlemongodb:
    container_name: articlemongodb
    image: mongo
    ports:
    - published: 27017
      target: 27017
    restart: always
    volumes:
    - mongo_data:/data/db:rw
  articles.api:
    build:
      context: C:\source\Csharp\AspnetMicroservices\src
      dockerfile: Services/Articles/Articles.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Articles.API
      target: base
    container_name: articles.api
    depends_on:
      articlemongodb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      MongoSettings:ConnectionString: mongodb://articlemongodb:27017
      NUGET_FALLBACK_PACKAGES: ''
    image: articlesapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/Articles.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8000
      target: 80
    tty: true
    volumes:
    - C:\source\Csharp\AspnetMicroservices\src\Services\Articles\Articles.API:/app:rw
    - C:\Users\alexe\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\alexe\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\alexe\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\alexe\.nuget\packages:/root/.nuget/packages:ro
    - C:\source\Csharp\AspnetMicroservices\src:/src:rw
  deliveries.api:
    build:
      context: C:\source\Csharp\AspnetMicroservices\src
      dockerfile: Services/Deliveries/Deliveries.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Deliveries.API
      target: base
    container_name: Deliveries.API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CacheSettings:ConnectionString: deliverydb:6379
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: deliveriesapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/Deliveries.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8502
      target: 80
    tty: true
    volumes:
    - C:\source\Csharp\AspnetMicroservices\src\Services\Deliveries\Deliveries.API:/app:rw
    - C:\Users\alexe\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\alexe\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\alexe\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\alexe\.nuget\packages:/root/.nuget/packages:ro
    - C:\source\Csharp\AspnetMicroservices\src:/src:rw
  deliverydb:
    container_name: deliverydb
    image: redis:alpine
    ports:
    - published: 6381
      target: 6379
    restart: always
  inventory.api:
    build:
      context: C:\source\Csharp\AspnetMicroservices\src
